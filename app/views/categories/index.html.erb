<h1 align="center" >All categories</h1>
<div class="center">
<%= will_paginate %>
<% @categories.each do |category| %>
	<ul class="row">
		<div class="well col-md-4 col-md-offset-4">
		<h2>
			<strong>
				<%= link_to "#{ category.name }", category_path(category) %>
			</strong>
		</h2>
		</div>
	</ul>
<% end %>
<%= will_paginate %>
</div>